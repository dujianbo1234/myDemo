export default {
  state: {
    layout: {},
    layoutWithWidgetData: []
  },
  mutations: {
    layout(state, payload) {
      state.layout = JSON.parse(JSON.stringify(payload));
    },
    layoutWithWidgetData(state, payload) {
      state.layoutWithWidgetData = JSON.parse(JSON.stringify(payload));
    },
  }
}